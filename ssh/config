# FNAL
Host ftbfbnl01
  HostName          ftbfbnl01.fnal.gov
  User              ftbf_user

Host 192.168.60.*
  ProxyJump         ftbfbnl01

Host *.fnal.gov
  GSSAPIAuthentication      yes
  GSSAPIDelegateCredentials yes

