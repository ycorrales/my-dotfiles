# LANL
Host ming_gateway
  Hostname          69.254.135.184
  User              maps
  Port              37023
  ProxyJump         none

Host lanl_flx
  Port              8022

Host lanl_vc709
  Port              8122

Host lanl_*
  Hostname          localhost
  User              maps
  ProxyJump         ming_gateway

Host mvtx_flx
  Hostname          204.121.146.106

Host eic_malta
  Hostname          204.121.146.105
  User              eic

Host vc709
  Hostname          204.121.146.104

#Host mosaic
#  Hostname          204.121.146.110

Host mvtx* eic* vc709*
  User              maps
  ForwardX11        yes
  ForwardX11Trusted yes
  ProxyJump         lanl_flx

